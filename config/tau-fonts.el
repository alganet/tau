(setenv "LANG" "en_US.UTF-8")
(set-language-environment "UTF-8")
(set-default-coding-systems 'utf-8)
(prefer-coding-system 'utf-8)

(set-default-font "Monoid Tight-11")
(set-face-attribute 'default t :font "Monoid Tight-11" )
(set-face-attribute 'default nil :font "Monoid Tight-11" )
(set-face-attribute 'variable-pitch t :font "Monoid Tight-9" )
(set-face-attribute 'variable-pitch nil :font "Monoid Tight-9" )


(provide 'tau-fonts)
